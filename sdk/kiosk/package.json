{
	"name": "@mysten/kiosk",
	"author": "Mysten Labs <build@mystenlabs.com>",
	"description": "Pera Kiosk library",
	"version": "0.9.16",
	"license": "BSD-3-Clause-Clear",
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"sideEffects": false,
	"files": [
		"CHANGELOG.md",
		"dist",
		"src"
	],
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "build-package",
		"test": "echo 'No unit tests for kiosk SDK'",
		"pre-commit": "pnpm prettier:fix && pnpm lint && pnpm build",
		"prepublishOnly": "pnpm build",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"test:e2e": "wait-on http://127.0.0.1:9123 -l --timeout 120000 && vitest run e2e",
		"prepare:e2e": "cargo build --bin pera --profile dev && cross-env RUST_LOG=warn,pera=error,anemo_tower=warn,consensus=off cargo run --bin pera -- start --with-faucet --force-regenesis"
	},
	"dependencies": {
		"@pera-io/pera": "workspace:*"
	},
	"devDependencies": {
		"@mysten/build-scripts": "workspace:*",
		"cross-env": "^7.0.3",
		"tmp": "^0.2.3",
		"ts-retry-promise": "^0.8.1",
		"typescript": "^5.5.3",
		"vite": "^5.3.3",
		"vitest": "^2.0.1",
		"wait-on": "^7.2.0"
	}
}
